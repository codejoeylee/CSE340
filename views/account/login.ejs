<!DOCTYPE html>
<html lang="en">
<head>
  <title><%= title %> | CSE Motors</title>
</head>
<body>
  <h1><%= title %></h1>

 

  <!-- Login Form -->
  <div class="login-container">
  <!-- Flash Message -->
  <%- messages() %>
  

  <!-- Login Form -->
  <form id="loginForm" action="/account/login" method="post">
    <label for="account_email">Email:</label>
    <input type="email" name="account_email" id="account_email" required>

    <label for="account_password">Password:</label>
    <input type="password" name="account_password" id="account_password" required>

    <button type="submit">Login</button>
  </form>

  <!-- Registration Link -->
  <p>Donâ€™t have an account? <a href="/account/register">Create one</a></p>
</div>
</body>
</html>
